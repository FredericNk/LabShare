{
    // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
    // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
    // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start Backend",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/backend/rest_api/main.ts",
            "preLaunchTask": "Compile TSC",
            "cwd": "${workspaceFolder}/",
            "outFiles": [
                "${workspaceFolder}/dist/backend/**/*.js"
            ],
            "env": {
                "NODE_PATH": "dist/backend/",
                "BASE_URL": "http://localhost:8080",
                "STAGING": "1",
            },
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Backend - Enable Discord Bot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/backend/rest_api/main.ts",
            "cwd": "${workspaceFolder}/",
            "outFiles": [
                "${workspaceFolder}/dist/backend/**/*.js"
            ],
            "env": {
                "NODE_PATH": "dist/backend/",
                "BASE_URL": "http://localhost:8080",
                "STAGING": "1",
                "DISABLE_DISCORD_BOT": "0",
            },
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Backend - Enable Mail",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/backend/rest_api/main.ts",
            "cwd": "${workspaceFolder}/",
            "env": {
                "NODE_PATH": "dist/backend/",
                "BASE_URL": "http://localhost:8080",
                "STAGING": "1",
                "ENABLE_MAIL": "1",
            },
            "outFiles": [
                "${workspaceFolder}/dist/backend/**/*.js"
            ]
        }
    ]
}